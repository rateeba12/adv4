       int i=1;
        while(i==1) {
            System.out.println("Enter three integers");
            System.out.println("rgb : ");

            Scanner myObj = new Scanner(System.in);  // Create a Scanner object
            int R = myObj.nextInt();  // Read user input
            int G;  // Read user input
            G = myObj.nextInt();
            int B = myObj.nextInt();  // Read user input
            Color c = new Color(R, G, B);

            //System.out.println(c.getRGB());
            System.out.println(c);

            Hash_Map cc = new Hash_Map();
            cc.z();
            cc.isKey(c);




            System.out.println("press 0 to exist and 1 to continue.");
            Scanner mObj = new Scanner(System.in);  // Create a Scanner object
            i = mObj.nextInt();

        }
        //  Color clickBoxColor = new Color(color.getRed(), color.getGreen(), color.getBlue(), 20);




















